	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"export": "next build && next export",
		"start": "next start",
		"lint": "eslint .",
		"prettier": "prettier --write . --loglevel silent",
		"format": "yarn prettier && yarn lint && yarn tsc",
		"test:cy": "cypress run --quiet",
		"test:cy:open": "cypress open",
		"test:cy:ci": "start-server-and-test dev http://localhost:3000 test:cy",
		"test:jest": "jest --silent --noStackTrace",
		"test:update": "jest --silent --noStackTrace --updateSnapshot",
		"test:coverage": "jest --coverage --coverageReporters=cobertura",
		"test:all": "yarn test:jest && yarn test:cy:ci",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"ci": "yarn format && yarn test:all"
	},
